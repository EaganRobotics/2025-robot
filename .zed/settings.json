{
  "lsp": {
    "jdtls": {
      "settings": {
        "classpath": "~/wpilib/2025/maven/edu/wpi/first/**/*.jar",
        "java": {
          "configuration": {
            "runtimes": [
              {
                "name": "JavaSE-17",
                "path": "~/wpilib/2025/jdk"
              }
            ],
            "updateBuildConfiguration": "automatic"
          },
          "gradle": {
            "enabled": "true",
            "downloadSources": "true"
          },
          "sources": {
            "organizeImports": {
              "starThreshold": 999,
              "staticStarThreshold": 999
            }
          },
          "format": {
            "settings": {
              "url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
              "profile": "GoogleStyle"
            }
          }
        }
      },
      "initialization_options": {
        "settings": {
          "java": {
            "project": {
              "sourcePaths": [
                "src/main/java"
              ],
              "referencedLibraries": [
                "~/wpilib/2025/maven/edu/wpi/first/**/*.jar"
              ]
            }
          }
        },
        "extendedClientCapabilities": {
          "classFileContentsSupport": true,
          "resolveAdditionalTextEditsSupport": true
        }
      }
    }
  },
  "tab_size": 2,
  "hard_tabs": false,
  "formatter": "language_server",
  "use_on_type_format": true,
  "format_on_save": "language_server",
  "tabs": {
    "git_status": true,
    "file_icons": true
  },
  "file_scan_exclusions": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/CVS",
    "**/.DS_Store",
    "**/.gradle/",
    "**/gradlew",
    "**/gradlew.bat",
    "bin/",
    "**/.classpath",
    "**/.project",
    "**/.settings",
    "**/.factorypath",
    "**/*~",
    "**/BuildConstants.java",
    "*.log",
    ".wpilib",
    "gradle",
    "ctre_sim",
    "build",
    "WPILib-License.md",
    ".kotlin",
    "**/simgui*.json",
    "**/networktables.json",
    "vendordeps"
  ]
}
